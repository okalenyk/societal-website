{
  "name": "societal-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "build-svg-sprite": "node scripts/generateIconsIndex.js",
    "stylelint": "stylelint --quiet --fix \"**/*.(scss|sass)\"",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "file-loader": "^6.2.0",
    "lottie-web": "^5.9.4",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-bodymovin": "^2.0.0",
    "react-dom": "18.1.0",
    "react-lottie": "^1.2.3",
    "react-mailchimp-subscribe": "^2.1.3",
    "swiper": "^8.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@storybook/addon-actions": "^6.5.0",
    "@storybook/addon-essentials": "^6.5.0",
    "@storybook/addon-interactions": "^6.5.0",
    "@storybook/addon-links": "^6.5.0",
    "@storybook/builder-webpack5": "^6.5.0",
    "@storybook/manager-webpack5": "^6.5.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.0",
    "@svgr/webpack": "^6.2.1",
    "@types/node": "17.0.34",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "@types/react-lottie": "^1.2.6",
    "@types/react-mailchimp-subscribe": "^2.1.1",
    "@types/svg-sprite-loader": "^3.9.5",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "eslint": "8.15.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-config-next": "12.1.6",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.2.0",
    "svg-sprite-loader": "^6.0.11",
    "typescript": "4.6.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --color --max-warnings 0 --no-ignore",
      "tsc-files --noEmit"
    ],
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --quiet --fix \"**/*.(scss|sass)\"",
      "prettier --write"
    ]
  },
  "standard-version": {
    "packageFiles": [],
    "bumpFiles": [],
    "skip": {
      "changelog": true,
      "commit": true
    }
  }
}
